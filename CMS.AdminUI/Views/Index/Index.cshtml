@{
    ViewBag.Title = "星联科技管理后台";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section submenu{
      
}
<div id="content">
    <div id="box">
        <h3>欢迎您：@ViewBag.UserName</h3>
        <table width="100%" cellpadding="5" cellspacing="5">
            <thead>
                <tr><th align="left" width="100">快捷菜单：</th><th align="right"><img src="/content/img/icons/clock.png" style="vertical-align:middle;" alt="clock" title="clock" />&nbsp;@DateTime.Now.ToString("yyyy年MM月dd日 HH:mm:ss")</th></tr>
            </thead>
			<tbody>
                <tr>
                    <td colspan="2">&nbsp;&nbsp;
                    <a href="#" target="_blank">
                        <img src="/content/img/icons/home.png" style="vertical-align:middle;" alt="星联科技" title="星联科技" />&nbsp;星联科技
                    </a>&nbsp;&nbsp;
                    </td>
                </tr>
                <tr><td colspan="2">&nbsp;&nbsp;<a href="/User/EditPwd"><img src="/content/img/icons/icon_key.gif" style="vertical-align:middle;" alt="修改密码" title="修改密码" />&nbsp;修改密码</a></td></tr>
                <tr><td colspan="2">&nbsp;&nbsp;<a href="/Login/Logout"><img src="/content/img/icons/exit.png" style="vertical-align:middle;" alt="退出系统" title="退出系统" />&nbsp;退出系统</a></td></tr>
			</tbody>
		</table>
    </div>
</div>
<script src="/Scripts/jquery-ui-1.8.11.min.js" type="text/javascript"></script>